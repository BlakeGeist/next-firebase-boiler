{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": "2020",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
        }
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": "error",
        "indent": ["error", 4]
    },
    "extends": ["prettier"]
}